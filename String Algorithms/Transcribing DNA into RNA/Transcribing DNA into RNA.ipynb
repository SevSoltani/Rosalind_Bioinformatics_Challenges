    "def DNA_to_RNA(file):\n",
    "    \n",
    "    file = open(file)\n",
    "    sequence = file.read().replace('\\n', '')\n",
    "    file.close()\n",
    "    RNA = sequence.replace('T', 'U')\n",
    "    \n",
    "    return RNA"
   ]
  },
